{
  "capabilities":
  [
    {
      "platformName": "Android",
      "deviceName": "Android Emulator",
      "platformVersion": "5.1",
      "browserName": "",
      "appActivity": "com.integralads.avid.testharness.AdConfigurationActivity",
      "fullReset": true
    }
  ],
  "configuration":
  {
    "cleanUpCycle":2000,
    "timeout":30000,
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "url":"http://localhost:4723/wd/hub",
    "maxSession": 1,
    "register": true,
    "registerCycle": 5000,
    "hubPort": <grid_port>,
    "hubHost": "<Grid_host_name_or_grid_ip-address>"
  }
}
